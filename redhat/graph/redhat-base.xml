<?xml version="1.0" standalone="no"?>
<graph>

	<description>
	Core graph for Stacki.
	</description>


	<!-- Graph Ordering for Kickstart Generation -->

	<order tail="HEAD">
		<head>pre-ssh-key</head>
	</order>

	<order head="ca">
		<tail>ssl-client</tail>
	</order>

	<order head="TAIL">
		<tail>postshell</tail>
	</order>


	<!-- Graph OO Framework -->

	<edge from="base">
		<to>rpc</to>
		<to>time-base</to>
		<to>postshell</to>
		<to>yum-base</to>
		<to>storage</to>
		<to>foundation-python</to>
		<to>uefi-base</to>
		<to>nsswitch</to>
	</edge>


	<order head="yum-base">
		<tail>repo-client</tail>
		<tail>repo-server</tail>
	</order>

	<order head="node" tail="storage"/>

</graph>

