[{{ alias }}]
name={{ name }}
{% if is_mirrorlist and is_mirrorlist != 0 %}
mirrorlist={{ url }}
{% else %}
baseurl={{ url }}
{% endif %}
{% if is_enabled and is_enabled != 1 %}
enabled={{ is_enabled }}
{% endif %}
{% if type and type is not none %}
type={{ type }}
{% endif %}

{% if autorefresh and autorefresh != 0 %}
autorefresh={{ autorefresh }}
{% endif %}
{% if assumeyes and assumeyes != 0 %}
assumeyes={{ assumeyes }}
{% endif %}
{% if gpgkey %}
gpgkey={{ gpgkey }}
{% endif %}

gpgcheck={{ gpgcheck }}

